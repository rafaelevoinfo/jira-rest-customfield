<atlassian-plugin key="org.thejaxx.jira.rest.plugin.thejaxx-rest-plugin" name="Jira Rest Values Plugin" plugins-version="2"><plugin-info><description>This plugin provides a custom field to be retrieved from url</description><version>1.0.0</version><vendor name="Yoni Moses" url="https://thejaxx.co.il/"/><param name="plugin-icon">images/plugin-icon.png</param><param name="plugin-logo">images/plugin-logo.png</param><param name="plugin-banner">images/plugin-banner.png</param><param name="vendor-icon">images/vendor-icon.png</param><param name="vendor-logo">images/vendor-logo.png</param><param name="configure.url">/plugins/servlet/com.thejaxx.jira.rest.plugin.admin/configuration</param></plugin-info><servlet name="Configuration Servlet" key="rest-configuration-servlet" class="com.thejaxx.jira.rest.plugin.admin.ConfigurationServlet"><description key="configuration-servlet.description">Configuration Servlet for Rest endpoints</description><url-pattern>/com.thejaxx.jira.rest.plugin.admin/configuration</url-pattern></servlet><customfield-type key="restfieldselection" name="Rest Values Selection Field" class="com.thejaxx.jira.rest.plugin.RestCustomField"><description>Allows selection of values from a rest resource.</description><resource type="velocity" name="view" location="templates/view.vm"/><resource type="velocity" name="edit" location="templates/edit.vm"/><resource type="download" name="customfieldpreview.png" location="images/preview.png"/></customfield-type><resource type="i18n" name="i18n" location="i18n"/><customfield-searcher key="restfieldsearcher" name="Rest Values Searcher" class="com.thejaxx.jira.rest.plugin.RestValuesSearcher"><description>Allow to search for rest values.</description><resource type="velocity" name="label" location="templates/search/rest-label-searcher.vm"/><resource type="velocity" name="search" location="templates/search/search-rest.vm"/><resource type="velocity" name="view" location="templates/search/search-view-rest.vm"/><valid-customfield-